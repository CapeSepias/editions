{
    "name": "editions-aws",
    "version": "0.1.0",
    "bin": {
        "aws": "bin/aws.js"
    },
    "scripts": {
        "compile": "tsc",
        "watch": "tsc -w",
        "cdk": "cdk",
        "synth": "cdk synth > cloudformation.yaml",
        "clean": "rm -v bin/*.js bin/*.d.ts lib/*.js lib/*.d.ts",
        "build": "run-s -ln compile synth clean"
    },
    "devDependencies": {
        "@types/node": "8.10.45",
        "aws-cdk": "^0.36.1",
        "typescript": "^3.5.2"
    },
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^0.36.1",
        "@aws-cdk/aws-cloudfront": "^0.36.1",
        "@aws-cdk/aws-iam": "^0.36.1",
        "@aws-cdk/aws-lambda": "^0.36.1",
        "@aws-cdk/aws-s3": "^0.36.1",
        "@aws-cdk/cdk": "^0.36.1",
        "@aws-cdk/core": "^0.36.1",
        "npm-run-all": "^4.1.5",
        "source-map-support": "^0.5.12"
    }
}
