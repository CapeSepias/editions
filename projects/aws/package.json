        "@aws-cdk/aws-cloudfront": "^1.4.0",
        "@aws-cdk/aws-iam": "^1.4.0",
        "@aws-cdk/aws-lambda": "^1.4.0",
        "@aws-cdk/aws-s3": "^1.4.0",
        "@aws-cdk/aws-stepfunctions": "^1.4.0",
        "@aws-cdk/aws-stepfunctions-tasks": "^1.4.0",
        "@aws-cdk/core": "^1.4.0",
        "npm-run-all": "^4.1.5",
        "source-map-support": "^0.5.13"
